	draw textline center=1 color=black font_bold=no font_family=mono font_italic=no font_size=26 html=yes show_if=always text="One or more of the responses was inaccurate.<br /><br />The numbers you were presented with were:<br /><br /><br /><br /><br /><br />When you got all three numbers right, the experiment can beginn.<br /><br /><br />Press any key to try again." x=0 y=0 z_index=0
	draw textline center=1 color=black font_bold=yes font_family=mono font_italic=no font_size=50 html=yes show_if=always text="5 6 9<br />" x=0 y=0 z_index=0

define sequence experiment
	set flush_keyboard yes
	set description "Runs a number of items in sequence"
	run DigitSpan_Instructions always
	run DigitSpan_FeedbackLoop always
	run Bridge always
	run DigitSpan_Loop always
	run End always

