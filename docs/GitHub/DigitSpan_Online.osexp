	set description "Runs a number of items in sequence"
	run DigitSpanDisplay1 always
	run DigitSpanDisplay2 always
	run DigitSpanDisplay3 always
	run DigitSpanDisplay4 "[NSequence]>3"
	run DigitSpanDisplay5 "[NSequence]>4"
	run DigitSpanDisplay6 "[NSequence]>5"
	run DigitSpanDisplay7 "[NSequence]>6"
	run DigitSpanDisplay8 "[NSequence]>7"
	run DigitSpanDisplay9 "[NSequence]>8"
